{
    "name": "lucinda/nosql-data-access",
    "type": "library",
    "description": "API abstracting communication with NoSQL key-value store databases through a PDO-like structure",
    "keywords": [
        "nosql",
        "driver",
        "redis",
        "couchbase",
        "memcache",
        "memcached",
        "apc",
        "apcu"
    ],
    "homepage": "https://github.com/aherne/php-nosql-data-access-api",
    "license": "MIT",
    "require": {
        "php": "^7.1|^8.0",
        "ext-SimpleXML": "*"
    },
    "require-dev": {
        "lucinda/unit-testing": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Lucinda\\NoSQL\\": "src/",
            "Lucinda\\NoSQL\\Vendor\\": "drivers/",
            "Test\\Lucinda\\NoSQL\\": "tests/",
            "Test\\Lucinda\\NoSQL\\Vendor\\": "tests_drivers/"
        }
    },
    "authors": [
        {
            "name": "Lucian Popescu",
            "email": "lucian.gabriel.popescu@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/unit-testing"
        }
    ]
}
